  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "html-docx-js": "^0.3.1",
    "htmx.org": "^1.9.10",
    "mammoth": "^1.8.0",
    "mongodb-memory-server": "^9.1.3",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1"
  }
}