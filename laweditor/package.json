{
  "name": "document-management-system",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "build:vitedoc": "cd ../vitedoc && npm run build",
    "setup": "npm install && cd ../vitedoc && npm install",
    "dev:all": "concurrently \"npm run dev\" \"cd ../vitedoc && npm run dev\""
